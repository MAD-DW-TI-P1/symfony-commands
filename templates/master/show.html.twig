{% extends 'base.html.twig' %}

{% block title %}Master{% endblock %}

{% block body %}
    <h1>Master</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ master.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ master.name }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{{ master.created ? master.created|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Alert</th>
                <td>{{ master.alert ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Start</th>
                <td>{{ master.start ? master.start|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_master_index') }}">back to list</a>

    <a href="{{ path('app_master_edit', {'id': master.id}) }}">edit</a>

    {{ include('master/_delete_form.html.twig') }}
{% endblock %}
